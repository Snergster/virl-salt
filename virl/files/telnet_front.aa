  # allow fronting telnet consoles for kvm
  /bin/bash rmix,
  /bin/sed rmix,
  /usr/bin/sort rmix,
  /usr/bin/gawk rmix,
  /usr/bin/xargs rmix,
  /sbin/ifconfig rmix,
  /usr/bin/telnet_front rmix,
  /usr/bin/socat rmix,
  /bin/sleep rmix,
  /proc/[0-9]*/cmdline r,
  /proc/[0-9]*/net/dev r,
  /proc/net/route r,
  network inet dgram,
